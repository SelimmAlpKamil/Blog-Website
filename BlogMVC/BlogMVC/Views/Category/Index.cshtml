@model IEnumerable<EntityLayer.Concrete.Category>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <table>
            <tr>
                <th>Id</th>
                <th>Ad</th>
            </tr>
            @foreach(var x in Model)
            {
                <tr>
                    <td>@x.CategoryId</td>
                    <td>@x.CategoryName</td>
                </tr>
            }
            

        </table>

    </div>
</body>
</html>
